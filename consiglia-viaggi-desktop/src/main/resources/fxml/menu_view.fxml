<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<ScrollPane fitToHeight="true" fitToWidth="true" styleClass="root" stylesheets="@../css/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <BorderPane fx:id="menuView" styleClass="border-pane" stylesheets="@../css/application.css">
         <center>
            <VBox alignment="CENTER" prefHeight="407.0" prefWidth="559.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#recensioni" prefHeight="50.0" prefWidth="150.0" text="Recensioni">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#strutture" prefHeight="50.0" prefWidth="150.0" text="Strutture">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
         <top>
            <Text fx:id="welcomeText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" BorderPane.alignment="CENTER">
               <font>
                  <Font size="33.0" />
               </font>
            </Text>
         </top>
         <bottom>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#logout" prefHeight="25.0" prefWidth="150.0" text="Logout" BorderPane.alignment="CENTER_RIGHT">
               <BorderPane.margin>
                  <Insets bottom="5.0" right="20.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
      </BorderPane>
   </content>
</ScrollPane>
